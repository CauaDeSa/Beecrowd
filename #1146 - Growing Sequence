#include <stdio.h>
 
int main(){
    int command, i;
    
    scanf("%d", &command);

    while (command != 0)
    {
        for (i = 0; i < command; i++)
        {
            printf("%d", i + 1);

            if(i < command - 1)
                printf(" ");
        }

        printf("\n");

        scanf("%d", &command);
    }

    return 0;
}
