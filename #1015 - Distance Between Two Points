#include <stdio.h>
#include <math.h>
 
int main() {
 
    double x1, x2, y1, y2, totalx, totaly;
    
    scanf("%lf%lf%lf%lf", &x1, &y1, &x2, &y2);
    
    totalx = (x2 - x1) * (x2 - x1);
    totaly = (y2 - y1) * (y2 - y1);
    
    totalx = sqrt(totalx + totaly);
    
    printf("%.4lf\n", totalx);
 
    return 0;
}
