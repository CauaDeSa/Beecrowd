#include <stdio.h>
 
int main() {
 
	int times = 6, positive = 0;
	float n, avg = 0;
	
	times = 6;
 
  while(times != 0){

      scanf("%f", &n);
      
      if(n > 0)
      {
        positive++;
        avg += n;
      }
      
      times--;
  }
	
	printf("%d valores positivos\n", positive);
	
	avg = avg / positive;
	
	printf("%.1f\n", avg);
 
  return 0;
}
