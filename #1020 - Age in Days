#include <stdio.h>
int main(){
    
    int anos, meses, dias, dias2;

    scanf("%d", &dias2);

    anos = dias2 / 365;
    meses = (dias2 % 365) / 30;
    dias = (dias2 % 365) % 30;
    
    printf("%d ano(s)\n", anos);
    printf("%d mes(es)\n", meses);
    printf("%d dia(s)\n", dias);
    return 0;
}
