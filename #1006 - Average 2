#include <stdio.h>

double weights(double A, double B, double C);

int main() {
 
    double A, B, C, MEDIA;
    
    scanf("%lf%lf%lf", &A, &B, &C);
     
    MEDIA = weights(A, B, C);
 
    printf("MEDIA = %.1f\n", MEDIA);
    
    return 0;
}

double weights(double A, double B, double C){
    
    double total;
    
    total = ((A * 2) + (B * 3) + (C * 5)) / 10;
  
    return total;
}
